{
  "name": "@empathy/react-wrapper",
  "version": "1.0.2",
  "description": "A proxy that transforms Vue components into React ones",
  "main": "dist/index.js",
  "types": "types/index.js",
  "scripts": {
    "serve": "parcel tests/demo/index.html",
    "test": "npm run test:unit && npm run test:e2e:ci",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test serve http://localhost:1234 cypress:open",
    "test:e2e:ci": "start-server-and-test serve http://localhost:1234 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless --browser chrome",
    "build": "tsc && npm pack",
    "lint": "eslint --fix src",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --no-verify --prerelease alpha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cbn-javieri@bitbucket.org/colbenson/react-wrapper.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/colbenson/react-wrapper#readme",
  "dependencies": {
    "react": "^16.9.0",
    "tslib": "^1.11.1",
    "vue": "^2.6.11",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@empathy/eslint-plugin-x": "^1.2.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "commitizen": "^4.1.2",
    "cypress": "^5.5.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.4",
    "parcel": "^1.12.4",
    "start-server-and-test": "^1.11.5",
    "ts-jest": "^26.0.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "prettier": "@empathy/eslint-plugin-x/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "npm run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
