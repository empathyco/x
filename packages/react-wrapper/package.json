{
  "name": "@empathy/react-wrapper",
  "version": "0.0.0",
  "description": "A proxy that transforms Vue components into React ones",
  "main": "dist/index.js",
  "types": "types/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc && npm pack",
    "lint": "eslint --fix src",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --no-verify --prerelease alpha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cbn-javieri@bitbucket.org/colbenson/react-wrapper.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/colbenson/react-wrapper#readme",
  "dependencies": {
    "react": "^16.9.0",
    "tslib": "^1.11.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jsdoc": "^25.4.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-tsdoc": "^0.2.5",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "prettier-plugin-organize-imports": "^1.0.4",
    "react-dom": "^16.9.0",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{ts,tsx}": [
      "npm run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
