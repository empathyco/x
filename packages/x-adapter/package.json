{
  "name": "@empathyco/x-adapter-next",
  "version": "0.1.1-alpha.0",
  "description": "A search client for the Empathy search API",
  "author": "Empathy Systems Corporation S.L.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/empathyco/x/tree/main/packages/x-adapter#readme",
  "keywords": [
    "search",
    "adapter",
    "client",
    "fetch"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empathyco/x.git",
    "directory": "packages/x-adapter"
  },
  "scripts": {
    "prebuild": "rimraf ./dist ./types",
    "build": "concurrently \"npm run build:*\"",
    "build:cjs": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "postbuild": "npm pack",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "tslib": "~2.3.0"
  },
  "devDependencies": {
    "concurrently": "~7.0.0",
    "rimraf": "~3.0.2",
    "typescript": "~4.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
