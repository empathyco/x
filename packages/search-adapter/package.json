{
  "name": "@empathyco/x-adapter",
  "version": "7.0.0-alpha.13",
  "description": "A search client for the Empathy search API ",
  "author": "Empathy Systems Corporation S.L.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/empathyco/x/tree/main/packages/search-adapter#readme",
  "keywords": [
    "search",
    "adapter",
    "client",
    "fetch"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "report"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/empathyco/x.git",
    "directory": "packages/search-adapter"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json && npm run gen:docs",
    "build:watch": "tsc --watch",
    "gen:model-docs": "api-extractor run -l",
    "gen:typescript-docs": "api-documenter markdown -i report -o docs",
    "gen:docs": "npm run gen:model-docs && npm run gen:typescript-docs",
    "postbuild": "npm pack",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@empathyco/x-deep-merge": "^1.3.0-alpha.2",
    "@empathyco/x-get-safe-property-chain": "^1.3.0-alpha.1",
    "@empathyco/x-logger": "^1.1.0",
    "@empathyco/x-storage-service": "^2.0.0-alpha.0",
    "@empathyco/x-types": "^10.0.0-alpha.17",
    "inversify": "~5.0.1",
    "reflect-metadata": "~0.1.13",
    "tslib": "~2.3.0"
  },
  "devDependencies": {
    "@empathyco/x-jest-utils": "^1.4.0-alpha.1",
    "@microsoft/api-documenter": "~7.15.3",
    "@microsoft/api-extractor": "~7.18.21",
    "@types/jest": "~27.0.3",
    "@types/node": "~16.4.12",
    "jest": "~27.3.1",
    "ts-jest": "~27.0.7",
    "typescript": "~4.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
