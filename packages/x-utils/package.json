{
  "name": "@empathyco/x-utils",
  "version": "1.0.3-alpha.6",
  "description": "A utility package for Empathy search",
  "author": "Empathy Systems Corporation S.L.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/empathyco/x/tree/main/packages/x-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empathyco/x.git",
    "directory": "packages/x-utils"
  },
  "keywords": [
    "search",
    "utils"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "prebuild": "rimraf ./dist ./types",
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json",
    "test": "jest",
    "test:unit": "jest",
    "postbuild": "pnpm pack",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "lint:inspect": "eslint --inspect-config .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@empathyco/x-storage-service": "^2.0.3-alpha.5",
    "tslib": "~2.8.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "jest": "29.7.0",
    "rimraf": "3.0.2",
    "ts-jest": "29.4.0",
    "typescript": "5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
