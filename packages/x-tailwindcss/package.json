{
  "name": "@empathyco/x-tailwindcss",
  "version": "2.0.0-alpha.20",
  "description": "Empathy X Tailwind plugin",
  "author": "Empathy Systems Corporation S.L.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/empathyco/x/tree/main/packages/x-tailwindcss#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empathyco/x.git",
    "directory": "packages/x-tailwindcss"
  },
  "keywords": [
    "tailwind",
    "design",
    "system",
    "empathy"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "showcase"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite serve demo",
    "dev:build": "vite build demo",
    "prebuild": "rimraf ./dist ./showcase ./*.tgz",
    "build": "rollup -c && vite build demo",
    "pack": "pnpm pack",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "lint:inspect": "eslint --inspect-config .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@empathyco/x-deep-merge": "^2.0.3-alpha.10",
    "@empathyco/x-utils": "^1.0.3-alpha.9",
    "tslib": "~2.8.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/vite": "4.1.17",
    "@rollup/plugin-commonjs": "29.0.0",
    "@vitejs/plugin-vue": "5.2.4",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.35",
    "postcss-import": "16.1.0",
    "rimraf": "3.0.2",
    "rollup": "4.9.1",
    "rollup-plugin-typescript2": "0.36.0",
    "tailwindcss": "4.1.17",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3",
    "vite": "6.4.1",
    "vue": "3.5.25"
  },
  "publishConfig": {
    "access": "public"
  }
}
