{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "projectFolder": "..",
  "mainEntryPointFilePath": "<projectFolder>/dist/types/index.d.ts",
  "bundledPackages": ["@empathyco/x-types", "@empathyco/x-adapter-platform"],
  "apiReport": {
    "enabled": true,
    "reportFolder": "<projectFolder>/dist/report"
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<projectFolder>/dist/report/<unscopedPackageName>.api.json"
  },
  "dtsRollup": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": true,
    "tsdocMetadataFilePath": "<projectFolder>/dist/report/tsdoc-metadata.json"
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-internal-missing-underscore": {
        "logLevel": "none",
        "addToApiReportFile": false
      }
    },
    "tsdocMessageReporting": {
      "tsdoc-unsupported-tag": {
        "logLevel": "none",
        "addToApiReportFile": false
      }
    }
  }
}
