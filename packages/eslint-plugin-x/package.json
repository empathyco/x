{
  "name": "@empathyco/eslint-plugin-x",
  "version": "1.4.0-alpha.7",
  "description": "ESLint, Prettier and standard-version rules and configurations",
  "author": "Empathy Systems Corporation S.L.",
  "license": "Apache-2.0",
  "homepage": "https://github.com/empathyco/x/tree/main/packages/eslint-plugin-x#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/**/*",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/empathyco/x.git",
    "directory": "packages/eslint-plugin-x"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "npm pack",
    "commit": "git-cz",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "eslint": "^7.1.0 <7.19.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsdoc": "^24.0.6",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-tsdoc": "^0.2.5",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.1.2",
    "standard-version": "^8.0.0"
  },
  "devDependencies": {
    "commitizen": "~4.1.2",
    "cz-conventional-changelog": "~3.2.0"
  },
  "peerDependencies": {
    "eslint": "^7.1.0",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0"
  },
  "prettier": "./prettier-config.js",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
