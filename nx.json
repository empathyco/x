{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "test"]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/showcase",
        "{projectRoot}/types",
        "{projectRoot}/report"
      ]
    }
  }
}
